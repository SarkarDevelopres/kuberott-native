{
  "expo": {
    "name": "KuberOTT",
    "slug": "KuberOTT",
    "scheme": "kuberott",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "package": "com.sarkardevs.KuberOTT"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-video",
      "expo-asset",
      "expo-font"
    ],
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c76a6801-9fba-4002-a674-37fb7a2fc005"
      }
    }
  }
}